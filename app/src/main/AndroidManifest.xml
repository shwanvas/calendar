<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="practice.shwanvas.calendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalendarAppExample">
        <activity android:name="practice.shwanvas.calendar.EventEditActivity"></activity>
        <activity android:name="practice.shwanvas.calendar.WeekViewActivity" />
        <activity android:name="practice.shwanvas.calendar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="practice.shwanvas.calendar.NotificationView"
            android:label="Details of notification"
            android:parentActivityName="practice.shwanvas.calendar.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="practice.shwanvas.calendar.MainActivity"/>
        </activity>
    </application>


</manifest>